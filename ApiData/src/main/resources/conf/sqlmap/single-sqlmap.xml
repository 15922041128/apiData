<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.singleDao">

	<select id="getRemoteApiByLocal" parameterType="java.lang.Integer" resultType="java.util.HashMap">
		select * from remoteApi where localApiID = #{localApiID}
	</select>
	
	<!--  
	<select id="getRemoteApi" parameterType="java.lang.String" resultType="java.util.HashMap">
		select * from s_remoteApi where service = #{service}
	</select>
	-->
	
	<update id="updateCnt" parameterType="java.util.HashMap">
		update s_remoteApi set count = #{count} where ID = ${ID}
	</update>
	
	<select id="getKey" resultType="java.util.HashMap">
		select * from t_key
	</select>
	
</mapper>